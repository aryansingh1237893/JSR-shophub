{
  "name": "shophub-mobile",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "build:android": "eas build --platform android",
    "build:ios": "eas build --platform ios",
    "build:web": "expo export:web",
    "submit:android": "eas submit --platform android",
    "submit:ios": "eas submit --platform ios"
  },
  "dependencies": {
    "expo": "^50.0.0",
    "expo-constants": "^16.0.0",
    "expo-status-bar": "^1.11.0",
    "expo-camera": "^14.0.0",
    "expo-image-picker": "^15.0.0",
    "expo-location": "^17.0.0",
    "expo-notifications": "^0.0.0",
    "expo-permissions": "^15.0.0",
    "expo-secure-store": "^13.0.0",
    "expo-sharing": "^12.0.0",
    "expo-document-picker": "^11.0.0",
    "react": "^18.2.0",
    "react-native": "^0.74.0",
    "react-navigation": "^6.1.9",
    "react-navigation-bottom-tabs": "^6.5.11",
    "react-navigation-drawer": "^6.6.8",
    "react-navigation-native": "^6.1.9",
    "react-navigation-stack": "^6.3.20",
    "react-native-gesture-handler": "^2.14.0",
    "react-native-reanimated": "^3.6.0",
    "react-native-safe-area-context": "^4.8.2",
    "react-native-screens": "^3.29.0",
    "react-native-vector-icons": "^10.0.0",
    "react-native-svg": "^14.0.0",
    "axios": "^1.6.0",
    "redux": "^4.2.1",
    "react-redux": "^8.1.3",
    "redux-thunk": "^2.4.2",
    "@stripe/stripe-react-native": "^1.14.0",
    "formik": "^2.4.5",
    "yup": "^1.3.3",
    "@react-native-async-storage/async-storage": "^1.21.0",
    "react-native-otp-textinput": "^2.1.0",
    "react-native-star-rating": "^1.1.0",
    "react-native-image-resizer": "^12.0.0",
    "react-native-document-scanner-plugin": "^3.0.0",
    "react-native-video": "^6.2.0",
    "react-native-uuid": "^2.0.0",
    "react-native-share": "^8.0.5",
    "react-native-push-notification": "^8.1.1",
    "twilio-client": "^1.12.2",
    "lottie-react-native": "^6.4.1"
  },
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@types/react": "~18.2.0",
    "@types/react-native": "^0.73.0",
    "babel-plugin-module-resolver": "^5.0.0",
    "typescript": "^5.3.0",
    "eas-cli": "^5.9.0"
  },
  "expo": {
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "Allow ShopHub to access your photos.",
          "cameraPermission": "Allow ShopHub to access your camera."
        }
      ],
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow ShopHub to access your location."
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/notification-icon.png",
          "color": "#ffffff"
        }
      ]
    ]
  }
}
